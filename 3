print("Please input length:")
input_number1 = int(input())
print("Please input hight:")
input_number2 = int(input())
print("Please input number of pieces:")
input_number3 = int(input())

if (input_number3%input_number2==0) and (input_number3%input_number1==0):
   print(f"{input_number1}, {input_number2}, {input_number3} -> yes")
else:
   print(f"{input_number1}, {input_number2}, {input_number3} -> no")