print("Please input 6-demensial number.")
input_number = input()
sum_of_first_three_numbers = int(input_number[0])+int(input_number[1])+int(input_number[2])
sum_of_second_three_numbers = int(input_number[3])+int(input_number[4])+int(input_number[5])
if sum_of_first_three_numbers==sum_of_second_three_numbers:
      print(f"{input_number}-> yes")
else:
      print(f"{input_number} -> no")